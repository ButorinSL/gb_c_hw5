int[] multiP(int[] mas1)
{
    int[] res = new int[mas1.Length / 2 + mas1.Length % 2];
    int iLastres = res.Length - 1;
    res[iLastres] = mas1[iLastres];
    for (int i = 0; i < mas1.Length / 2; i++)
    {
        res[i] = mas1[i] * mas1[mas1.Length - 1 - i];
    }
